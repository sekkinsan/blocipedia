<h1> Public Jikis </h1>
<div class="row">
<% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%=link_to wiki.title, wiki %>
      </h4>
  </div>
<% end %>

  <div class="col-md-4">
    <%= link_to "New Jiki", new_wiki_path, class: 'btn btn-success' %>
  </div>
</div>